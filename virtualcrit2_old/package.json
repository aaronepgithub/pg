{
  "devDependencies": {
    "cordova-plugin-vibration": "^2.1.6",
    "cordova-plugin-background-geolocation": "@mauron85/cordova-plugin-background-geolocation"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-device": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-compat": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ble-central": {
        "BLUETOOTH_USAGE_DESCRIPTION": "Required to display HR and Speed"
      },
      "cordova-plugin-geolocation": {},
      "cordova-plugin-background-geolocation": {
        "GOOGLE_PLAY_SERVICES_VERSION": "11+",
        "ANDROID_SUPPORT_LIBRARY_VERSION": "26+",
        "ALWAYS_USAGE_DESCRIPTION": "App requires ...",
        "MOTION_USAGE_DESCRIPTION": "App requires motion detection",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME"
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "dependencies": {
    "@mauron85/cordova-plugin-background-geolocation": "3.1.0",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.1.0",
    "cordova-plugin-ble-central": "1.2.4",
    "cordova-plugin-compat": "1.2.0",
    "cordova-plugin-device": "2.0.3",
    "cordova-plugin-dialogs": "2.0.2",
    "cordova-plugin-file": "^4.3.3",
    "cordova-plugin-geolocation": "git+https://github.com/apache/cordova-plugin-geolocation.git",
    "cordova-plugin-network-information": "2.0.2",
    "cordova-plugin-statusbar": "2.4.3"
  }
}